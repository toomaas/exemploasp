@model IEnumerable<exemploasp.Models.Disponibilidade>

@{
    ViewBag.Title = "Ver";
}

<h2>Ver</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Exposicao.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserAccount.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataDisponibilidade)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Exposicao.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserAccount.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataDisponibilidade)
            </td>
        </tr>
    }
</table>
<div>
    @Html.ActionLink("Voltar atrás", "PerfilUser", "Account", new { id = Session["UserAccountID"] }, null)
</div>
