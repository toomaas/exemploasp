@model List<exemploasp.Models.UserAccount>

@{
    ViewBag.Title = "Funcao";
}

<h2>Adicionar Função a Utilizador</h2>
<hr>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">


        <div class="form-group">

            <label class="control-label col-md-2" for="UserAccountID">Utilizador</label>
            <div class="col-md-10">
                @Html.DropDownList("UserAccountID", "--Selecionar--")

            </div>

        </div>


        <div class="form-group">

            <label class="control-label col-md-2" for="TipoUtilizadorID">Função</label>
            <div class="col-md-10">
                @Html.DropDownList("TipoUtilizadorID", "--Selecionar--")
            </div>

        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Definir função" class="btn btn-default" />
            </div>
        </div>


    </div>
}
